<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomSeekBar">
        <attr name="numText" format="string"/>
        <attr name="numbackground" format="reference"/>
        <attr name="numTextSize" format="dimension"/>
        <attr name="numTextColor" format="color"/>
        <attr name="numType">
            <enum name="top" value="0"/>
            <enum name="bottom" value="1"/>
        </attr>

    </declare-styleable>
</resources>